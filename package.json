{
	"name": "mapshaper",
	"version": "0.5.26",
	"description": "A tool for editing vector datasets for mapping and GIS.",
	"keywords": [
		"shapefile",
		"topojson",
		"geojson",
		"cartography",
		"simplification",
		"topology",
		"gis"
	],
	"author": "Matthew Bloch <masiyou@gmail.com>",
	"contributors": [],
	"license": "MPL-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/mbloch/mapshaper"
	},
	"engines": {
		"node": ">=6.0.0"
	},
	"scripts": {
		"test": "mocha -r esm --parallel --jobs 4 --check-leaks -R dot",
		"build": "rollup --config",
		"postinstall": "npm run-script build",
		"prepublishOnly": "npm test",
		"postpublish": "./release",
		"browserify": "browserify -r mproj -r buffer -r iconv-lite -r fs -r flatbush -r rw -r path -o www/modules.js",
		"watch": "rollup --config --watch"
	},
	"main": "./index.js",
	"files": [
		"/bin/**",
		"/www/**",
		"!/www/nacis/",
		"/mapshaper.js",
		"!.DS_Store"
	],
	"dependencies": {
		"commander": "^5.1.0",
		"cookies": "^0.8.0",
		"flatbush": "^3.2.1",
		"iconv-lite": "0.4.24",
		"mproj": "0.0.26",
		"opn": "^5.3.0",
		"rollup": "^2.28.2",
		"rw": "~1.3.3",
		"sync-request": "5.0.0"
	},
	"devDependencies": {
		"browserify": "^16.5.0",
		"csv-spectrum": "^1.0.0",
		"deep-eql": ">=0.1.3",
		"esm": "^3.2.25",
		"mocha": "^8.1.1",
		"shell-quote": "^1.6.1",
		"underscore": "^1.9.0"
	},
	"bin": {
		"mapshaper": "./bin/mapshaper",
		"mapshaper-gui": "./bin/mapshaper-gui",
		"mapshaper-xl": "./bin/mapshaper-xl"
	}
}
